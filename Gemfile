## UPDATE GEMS WITH COMMAND 'bundle'

# frozen_string_literal: true

source "https://rubygems.org"
gemspec

# for demo site
gem "jekyll-paginate"

gem "jekyll"

# https://stackoverflow.com/questions/65989040/bundle-exec-jekyll-serve-cannot-load-such-file
gem "webrick"

# https://github.com/vwochnik/jekyll-email-protect
gem 'jekyll-email-protect'

# https://github.com/jekyll/jekyll-sitemap
gem 'jekyll-sitemap'

# https://github.com/lazee/premonition
gem 'premonition'

# https://github.com/helaili/jekyll-action/issues/150
gem "jekyll-sass-converter", "~> 2.0"